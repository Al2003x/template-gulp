{
  "name": "template-gulp",
  "version": "1.0.0",
  "description": "Simple gulp template",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"src/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint && eslint source/js/",
    "build": "gulp build",
    "start": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Al2003x/template-gulp.git"
  },
  "author": "Al2003x",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Al2003x/template-gulp/issues"
  },
  "homepage": "https://github.com/Al2003x/template-gulp#readme",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "^1.0.0",
    "autoprefixer": "^10.4.2",
    "browser-sync": "^2.27.7",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-dart-sass": "^1.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-libsquoosh": "^1.0.12",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-posthtml": "^3.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-svgmin": "^4.1.0",
    "gulp-svgstore": "^9.0.0",
    "gulp-terser": "^2.1.0",
    "postcss": "^8.4.6",
    "postcss-csso": "^6.0.0",
    "stylelint": "^14.5.1",
    "stylelint-config-htmlacademy": "^0.1.12"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not < 0.25%"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "src/*.html",
    "src/js/**/*.js",
    "src/img/**/*.svg",
    "src/sass/**/*.scss"
  ],
  "engines": {
    "node": "16.13.0"
  },
  "type": "module",
  "dependencies": {
    "normalize.css": "^8.0.1",
    "swiper": "^8.0.6"
  }
}
